@inherits R_Page
@attribute [R_Page(Title = "Withholding Tax")]

<R_StackLayout Style="width: 800px">
    <R_StackLayout Row>
        <R_Grid @ref="@GridRef"
                DataSource="_viewModel.WithholdingTaxGrid"
                R_ServiceGetListRecord="R_ServiceGetListRecordAsync"
                Pageable
                PageSize="5">
            <R_GridColumns>
                <R_GridTextColumn FieldName="CTAX_TYPE_DESCR" HeaderText="Tax Type"></R_GridTextColumn>
            </R_GridColumns>
        </R_Grid>
    </R_StackLayout>

        <R_StackLayout Row Class="justify-content-end">

            <R_Button OnClick="Button_OnClickOkAsync">
                OK
            </R_Button>
            <R_Button OnClick="Button_OnClickCloseAsync">
                Cancel
            </R_Button>
    </R_StackLayout>

</R_StackLayout>