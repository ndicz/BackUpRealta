     @page "/GSM05510"
     @inherits R_Page



         <R_StackLayout>

                

             <R_Grid @ref="@_gridRef5510"
                     DataSource="GSM05510ViewModel.loGridList"
                     Pageable="true"
                     R_ConductorGridSource="@_conGridGSM05510Ref"
                     R_GridType="@R_eGridType.Original"
                     AllowAddNewRow="true"
                     Width="1080px"
                     AllowEditRow="true"
                     AllowDeleteRow="true"
                     R_ServiceGetListRecord="@Grid_R_ServiceGetListRecordRateType">

                 <R_GridColumns>

                     <R_GridTextColumn FieldName="CRATETYPE_CODE" HeaderText="Rate Type Code" R_EnableAdd></R_GridTextColumn>
                     <R_GridTextColumn FieldName="CRATETYPE_DESCRIPTION" HeaderText="Rate Type Name" R_EnableAdd R_EnableEdit></R_GridTextColumn>
                     <R_GridTextColumn FieldName="CUPDATE_BY" HeaderText="Last Updated By" />
                     <R_GridTextColumn FieldName="DUPDATE_DATE" HeaderText="last Updated Date" />
                     <R_GridTextColumn FieldName="CCREATE_BY" HeaderText="Create By" />
                     <R_GridTextColumn FieldName="DCREATE_DATE" HeaderText="Created Date" />

                 </R_GridColumns>
             </R_Grid>

         </R_StackLayout>
         <R_ConductorGrid @ref="@_conGridGSM05510Ref"
                          R_ViewModel="@GSM05510ViewModel"
                          R_ServiceGetRecord="@Grid_ServiceGetRecordRateType"
                          R_ServiceSave="@Grid_ServiceSaveRateType"
                          R_ServiceDelete="@Grid_ServiceDeleteRateType"
                          @*R_AfterDelete="@Grid_AfterDelete"*@
                          R_IsHeader="true" />
